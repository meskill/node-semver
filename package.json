{
  "name": "semver",
  "version": "7.3.5",
  "description": "The semantic version parser used by npm.",
  "main": "index.js",
  "scripts": {
    "test": "tap",
    "snap": "tap",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git push origin --follow-tags",
    "lint": "eslint '**/*.js'",
    "postlint": "npm-template-check; exit 0",
    "template-copy": "npm-template-copy --force",
    "lintfix": "npm run lint -- --fix",
    "prepublishOnly": "git push origin --follow-tags",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@npmcli/template-oss": "2.8.1",
    "tap": "^15.1.6"
  },
  "license": "ISC",
  "repository": "https://github.com/npm/node-semver",
  "bin": {
    "semver": "bin/semver.js"
  },
  "files": [
    "bin/**/*.js",
    "range.bnf",
    "classes/**/*.js",
    "functions/**/*.js",
    "internal/**/*.js",
    "ranges/**/*.js",
    "index.js",
    "preload.js"
  ],
  "tap": {
    "check-coverage": true,
    "coverage-map": "map.js"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "lru-cache": "^6.0.0"
  },
  "author": "GitHub Inc.",
  "templateOSS": {
    "version": "2.8.1"
  }
}
